<div class="overlay">
    <h1>{{blurb}}</h1>
    <h2>{{first_name}} {{last_name}}</h2>

    <p class="tag">Pet my little furry head.</p>

    <div class='icons'>
        <a href="#" class="twitter">{{twitter_handle}}</a>
        <a href="#" class="email">{{email}}</a>
        <a href="#" class="link">URL</a>
    </div>

    <div class="tags">
        {{#each skills}}
          <a href="#">{{this}}</a>
        {{/each}}
    </div>

</div>
